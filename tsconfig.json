{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6", "dom", "es2017"],
    "module": "commonjs",
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"]
    },
    "resolveJsonModule": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
